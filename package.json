  "description": "Online store",
  "main": "src/server.js",
  "scripts": {
    "test": "jest --runInBand",
    "start": "ts-node-dev -r tsconfig-paths/register --respawn --pretty --transpile-only src/server.ts",
    "build": "rm -rf build/ && prettier --write src/ && tsc",
    "seed-database": "ts-node-dev -r tsconfig-paths/register --pretty --transpile-only src/Scripts/seedDatabase.ts"
  },
  "author": "Ali Amini<aliamini.code@gmail.com>",
  "license": "ISC",
  "_moduleAliases": {
    "@middlewares": "src/middlewares",
    "@services": "src/services",
    "@helpers": "src/helpers",
    "@models": "src/models",
    "@controllers": "src/controllers",
    "@mytypes": "src/@types",
    "@routes": "src/api/v1",
    "@src": "src"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.5",
    "morgan": "^1.10.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^28.1.0",
    "@types/node": "^17.0.38",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0"
  }
